<div class="content-wrapper pb-0">
    <div class="page-header flex-wrap">
        <h3 class="mb-0"> Hi {{user.name}}, welcome back!</h3>
        <div class="d-flex"></div>
    </div>
<!--    Manage users-->
    <div class="row">
        <div class="col-xl-12 col-sm-12 grid-margin stretch-card">
            <div class="card">
                <div class="card-body px-0 overflow-auto">
                    <h4 class="card-title pl-4">
                        Manage users
                    </h4>

                    <div class="table-responsive">
                        <table class="table">
                            <thead class="bg-light">
                            <tr>
                                <th>Username</th>
                                <th>Name</th>
                                <th>Email</th>
                                <th>Status</th>
                                <th></th>
                            </tr>
                            </thead>
                            <tbody>
                            {{#each userList}}
                                {{#if userType}}

                                {{else}}
                                    <tr>
                                        <td>
                                            <div class="d-flex align-items-center">
                                                <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSng8Jejbfct93auTLtTP08eTd5yXV_j_dh0Q&usqp=CAU" alt="image" />
                                                <div class="table-user-name ml-3">
                                                    <p class="mb-0 font-weight-medium">{{username}}</p>
                                                    <small> Online</small>
                                                </div>
                                            </div>
                                        </td>
                                        <td>{{name}}</td>
                                        <td>{{email}}</td>
                                        <td>
                                            {{#if status}}
                                                <div class="badge badge-inverse-success"> normal </div>
                                            {{else}}
                                                <div class="badge badge-inverse-success"> locked </div>
                                            {{/if}}
                                        </td>
                                        <td>
                                            <div class="dropdown">
                                                <button class="btn btn-warning dropdown-toggle" type="button" id="dropdownMenuIconButton1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                    <i class="mdi mdi-earth"></i>
                                                </button>
                                                <div class="dropdown-menu" aria-labelledby="dropdownMenuIconButton1">
                                                    <h6 class="dropdown-header">Settings</h6>
                                                    <a class="dropdown-item" href="/admin/userProfile/{{_id}}">View</a>
                                                    <a class="dropdown-item" href="/admin/updateProfile/{{_id}}">Edit</a>
                                                    <a class="dropdown-item" href="/admin/user/delete/{{_id}}">Delete</a>
                                                    <a class="dropdown-item" href="/admin/user/lock/{{_id}}">Lock</a>
                                                    <a class="dropdown-item" href="/admin/user/unlock/{{_id}}">Unlock</a>>
                                                </div>
                                            </div>
                                        </td>
                                    </tr>
                                {{/if}}
                            {{/each}}
                            </tbody>
                        </table>
                    </div>
                    <a class="text-black mt-3 d-block pl-4" href="admin/user">
                        <span class="font-weight-medium h6">View all users</span>
                        <i class="mdi mdi-chevron-right"></i></a>
                </div>
            </div>
        </div>
