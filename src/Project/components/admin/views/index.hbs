    <div class="row">
        <div class="col-xl-12 col-sm-12 grid-margin stretch-card">
            <div class="card">
                <div class="card-body px-0 overflow-auto">
                    <h4 class="card-title pl-4">
                        Manage movies
                        <a href="/admin/addMovie"><button type="button" class="btn btn-sm ml-3 btn-success"> Add Movie </button></a>
                        <a href="/admin/movie"><button type="button" class="btn btn-sm ml-3 btn-success"> View Movie List </button></a>
                    </h4>
                    <div class="table-responsive">
                        <table class="table">
                            <thead class="bg-light">
                            <tr>
                                <th>Movie</th>
                                <th>Genre</th>
                                <th>Status</th>
                                <th>Views</th>
                                <th>Author</th>
                                <th>Update</th>
                            </tr>
                            </thead>
                            <tbody>
                            {{#each movieList}}
                                <tr>
                                    <td>
                                        <div class="d-flex align-items-center">
                                            <img src="{{this.images.[0]}}" alt="movie {{this.movieName}} image" />
                                            <div class="table-user-name ml-3">
                                                <p class="mb-0 font-weight-medium"> {{this.movieName}} </p>
                                                <small> {{this.type}}</small>
                                            </div>
                                        </div>
                                    </td>
                                    <td>{{this.genre}}</td>
                                    <td>
                                        <div class="badge badge-inverse-success"> {{this.episode}} </div>
                                    </td>
                                    <td> 140</td>
                                    <td>{{this.author}}</td>
                                    <td>{{this.update}}</td>
                                </tr>
                            {{/each}}
                            </tbody>
                        </table>
                    </div>
                    <a class="text-black mt-3 d-block pl-4" href="#">
                        <span class="font-weight-medium h6">View all movies</span>
                        <i class="mdi mdi-chevron-right"></i></a>
                </div>
            </div>
        </div>
